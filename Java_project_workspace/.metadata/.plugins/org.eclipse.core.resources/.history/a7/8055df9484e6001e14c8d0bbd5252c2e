package com.society.controller;

import java.util.List;

import org.apache.catalina.servlet4preview.http.HttpServletRequest;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PostMapping;
import org.springframework.web.bind.annotation.RequestParam;
import org.springframework.web.bind.annotation.RestController;
import org.springframework.web.multipart.MultipartFile;
import org.springframework.web.servlet.ModelAndView;

@Controller
@RestController
public class MembersController {

	@GetMapping(value = "/members")
    public ModelAndView manageOwners() {
        return new ModelAndView("admin/manageMembers");
    }
	
	@PostMapping(value = "/saveMembers")
    public ModelAndView saveMembers(@RequestParam List<MultipartFile> memberFile, HttpServletRequest request) {
		
		String memberNames = request.getParameterValues("");
		String memberPhoneNumber = request.getParameterValues("");
		String memberEmailAddress = request.getParameterValues("");
		
        return new ModelAndView("redirect:members");
    }

}
